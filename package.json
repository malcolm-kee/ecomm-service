{
  "name": "ecomm-service",
  "version": "0.0.1",
  "description": "",
  "author": "Malcolm Kee <malcolm.keeweesiong@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "concurrently \"node dist/main\" \"node seed\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:fake": "webpack --config webpack.fake.config.js",
    "dev:fake": "webpack --config webpack.fake.config.js --watch",
    "generate": "node dist-fake/build.js",
    "generate:local": "cross-env IS_DEV=true node dist-fake/build.js",
    "seed": "node seed",
    "heroku-postbuild": "npm run build && npm run build:fake && npm run generate"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/platform-ws": "^7.4.4",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/websockets": "^7.4.4",
    "blurhash": "^1.1.3",
    "bufferutil": "^4.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "concurrently": "^5.2.0",
    "date-fns": "^2.16.1",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-webp": "^6.0.0",
    "lodash": "^4.17.15",
    "mime-db": "^1.45.0",
    "mongodb-memory-server": "^6.9.2",
    "mongoose": "^5.10.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "superagent": "^6.1.0",
    "swagger-ui-express": "^4.1.4",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.2",
    "@types/glob": "^7.1.1",
    "@types/imagemin": "^7.0.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.162",
    "@types/markdown-it": "^10.0.1",
    "@types/mongoose": "^5.7.21",
    "@types/ncp": "^2.0.4",
    "@types/node": "^14.11.10",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.5",
    "@types/rimraf": "^3.0.0",
    "@types/sharp": "^0.26.0",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.1.0",
    "file-type": "^16.0.0",
    "glob": "^7.1.6",
    "jest": "^26.6.0",
    "markdown-it": "^12.0.1",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "prettier": "^2.1.2",
    "request": "^2.88.2",
    "sharp": "^0.26.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.6",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^2.5.2",
    "webpack-source-map-support": "^2.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
